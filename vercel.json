{
    "version": 2,
    "name": "sistema-seguimiento-alumnos",
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    },
    "regions": [
        "gru1"
    ]
}